{
  "manifest_version": 3,
  "name": "Demo Navigator",
  "version": "1.0",
  "description": "Navigate through demo links based on selected criteria.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/cs_logo.png",
      "48": "icons/cs_logo.png",
      "128": "icons/cs_logo.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "icons/cs_logo.png",
    "48": "icons/cs_logo.png",
    "128": "icons/cs_logo.png"
  }
}
